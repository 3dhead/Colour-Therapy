		<li class="logo"><%= link_to "CT", root_path%></li>

		<li><%= link_to "Categories", categories_path %> </li>

		<li><%= link_to "Gallery", root_path %></li>

		<li><% if current_user %>
				<%= link_to current_user.name, account_path %>
			<% else %>
				<%= link_to "Log In", login_path %>
			<% end %>
		</li>

	</ul>
</nav>
</div>

<div id="wrapper">
<%= yield %>



</body>
</html>

